<?xml version='1.0'?>
<robot name="fred">
  <gazebo reference="range_sensor">
    <sensor name="range_sensor" type="radar">
      <visualize>true</visualize>
      <ray>
        <range>
          <min>0.20</min>
          <max>1.0</max>
          <resolution>0.001</resolution>
        </range>
      </ray>
      <plugin name="range_sensor_plugin" filename="libgazebo_ros_range.so">
      </plugin>
    </sensor>
  </gazebo>
</robot>
