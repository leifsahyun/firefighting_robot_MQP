<?xml version='1.0'?>

  <robot name="FRED">

    <link name='chassis'>
      <pose>0 0 .1 0 0 0</pose>

      <collision>
        <geometry>
          <box size=".3048 .3048 .1524">
          </box>
        </geometry>
      </collision>

      <visual>
        <geometry>
          <box size=".3048 .3048 .1524">
          </box>
        </geometry>
      </visual>
      
      <collision name='tail_collision'>
        <origin xyz="-.1524 0 -.0762" rpy="0 0 0"/>
        <geometry>
          <polyline>
            <point>0 -.1524</point>
            <point>0 .1524</point>
            <point>-.3048 0</point>
            <height>.0762</height>
          </polyline>
        </geometry>
      </collision>
      
      <visual name='tail_visual'>
        <pose>-.1524 0 -.0762 0 0 0</pose>
        <geometry>
          <polyline>
            <point>0 -.1524</point>
            <point>0 .1524</point>
            <point>-.3048 0</point>
            <height>.0762</height>
          </polyline>
        </geometry>
      </visual>
      
      <collision name='caster_collision'>
        <pose>-0.3862 0 -0.05 0 0 0</pose>
        <geometry>
            <sphere>
            <radius>.05</radius>
          </sphere>
        </geometry>

        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>

      <visual name='caster_visual'>
        <pose>-0.3862 0 -0.05 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>.05</radius>
          </sphere>
        </geometry>
      </visual>
    </link>
    
    <link name="left_wheel">
      <pose>.1016 .1624 .0635 0 1.5707 1.5707</pose>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>.0635</radius>
            <length>.02</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>.0635</radius>
            <length>.02</length>
          </cylinder>
        </geometry>
      </visual>
    </link>
    
    <link name="right_wheel">
      <pose>.1016 -.1624 .0635 0 1.5707 1.5707</pose>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>.0635</radius>
            <length>.02</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>.0635</radius>
            <length>.02</length>
          </cylinder>
        </geometry>
      </visual>
    </link>
    
    <joint type="revolute" name="left_wheel_hinge">
      <pose>0 0 -0.01 0 0 0</pose>
      <child>left_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint type="revolute" name="right_wheel_hinge">
      <pose>0 0 0.01 0 0 0</pose>
      <child>right_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>
    
    <joint type="fixed" name="range_joint">
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
      <pose> 0 0 0 0 0 0 </pose>
      <parent>chassis</parent>
      <child>range_sensor</child>
    </joint>
    
    <link name="range_sensor">
      <collision name="collision">
        <pose> .1524 0 .1524 0 0 0 </pose>
        <geometry>
          <box>
            <size>.01 .01 .01</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <pose> .1524 0 .1524 0 0 0 </pose>
        <geometry>
          <box>
            <size>.01 .01 .01</size>
          </box>
        </geometry>
      </visual>
      <sensor name="range_sensor" type="radar">
        <visualize>true</visualize>
        <ray>
          <range>
            <min>0.20</min>
            <max>1.0</max>
            <resolution>0.001</resolution>
          </range>
        </ray>
        <plugin name="range_sensor_plugin" filename="libgazebo_ros_range.so">
        </plugin>
      </sensor>
    </link>
    
  </robot>

